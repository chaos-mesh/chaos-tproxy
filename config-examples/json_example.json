{
  "listen_port": 58080,
  "proxy_ports": [80, 8080, 30086],
  "proxy_mark": 1,
  "ignore_mark": 255,
  "rules": [
    {
      "target": "Request",
      "selector": {
        "path": "/example",
        "method": "GET"
      },
      "actions": {
        "delay": "10s",
        "append": {
          "queries": [
            ["foo", "bar"],
            ["foo", "other"]
          ]
        }
      }
    },
    {
      "target": "Response",
      "selector": {
        "path": "/example",
        "method": "GET",
        "code": 404
      },
      "actions": {
        "abort": true
      }
    }
  ]
}
